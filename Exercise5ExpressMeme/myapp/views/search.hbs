 <h1>Search Meme</h1>
    <div class="container">
        <form action="/searchMeme" enctype="multipart/form-data" method="POST">
            <label for="memeTitle">Title</label>
            <input type="text" id="memeTitle" name="memeTitle" placeholder="The title of the meme">

            <select name="genreSelect" id="genreSelect">
                {{#each tags}}
        <option value="{{this}}">{{this}}</option>  
            {{/each}}
                
            </select>
            <input type="submit" value="Submit">

        </form>
    </div>